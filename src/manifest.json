{
  "manifest_version": 3,
  "name": "Pinboard++ for Pinboard",
  "version": "2.0.0",
  "description": "Enhance your Pinboard experience.",
  "author": "cppcho",
  "minimum_chrome_version": "88",
  // "icons": {
  //   "16": "img/icon-16.png",
  //   "48": "img/icon-48.png",
  //   "128": "img/icon-128.png"
  // },
  "action": {
    // "default_icon": {
    //   "19": "img/ba-19.png",
    //   "38": "img/ba-38.png"
    // },
    "default_title": "Pinboard++",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  // "background": {
  //   "scripts": [
  //     "js/lib/jquery.min.js",
  //     "js/eventPage.js"
  //   ],
  //   "persistent": false
  // },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      }
    }
  }
}
